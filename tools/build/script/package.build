<?xml version="1.0" encoding="utf-8"?>
<project name="Ecm" xmlns="http://nant.sf.net/release/0.85/nant.xsd" basedir=".">

    <target name="package.execute">

		<if test="${target::exists('package.before')}">
			<call target="package.before" />
		</if>

		<copy todir="${package.dir}">
			<fileset refid="release.files" />
		</copy>

		<echo message="" />
		<echo message="Pacotes de distribuição gerados na pasta ${package.dir}" />
		<echo message="" />
		
    </target>

</project>